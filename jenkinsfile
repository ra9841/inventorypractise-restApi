pipeline {
    agent any

    tools {
        // This 'maven' must match the Maven tool configuration in your Jenkins.
        maven 'rabin'
    }

    stages {
        stage('Build') {
            steps {
                // Run Maven.
                sh 'mvn clean package'
                echo "packaging"
                echo 'ngrok testing...'
                echo 'Testing webhook'
            }
        }

        stage('Test') {
            steps {
                sh 'mvn test'
                echo 'Testing...'
            }
        }

        }
    }
}
